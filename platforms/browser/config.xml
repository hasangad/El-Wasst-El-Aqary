<?xml version='1.0' encoding='utf-8'?>
<widget id="com.SHCEMPSERV" version="1.0.4" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>My SHC</name>
    <description>
تطبيق خدمات منسوبي المجلس الصحي السعودي   </description>
    <author email="hasan.gadallah@gmail.com" href="http://mobile.shc.gov.sa/">
    SHC IT team
  </author>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="BackgroundColor" value="#089e7a" />
    <preference name="fullscreen" value="true" />
    <preference name="phonegap-version" value="cli-8.0.0" />
    <preference name="pgb-builder-version" value="2" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="target-device" value="universal" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-installLocation" value="auto" />
</widget>
